create postgres DB
- use sequelize js to connect to the app
- deploy with heroku


CREATE TABLE PRE(
   ID INT PRIMARY KEY NOT NULL,
   FULL_NAME TEXT NOT NULL,
   EMAIL TEXT NOT NULL,
);


CREATE TABLE INTERVIEW(
   ID INT PRIMARY KEY NOT NULL,
   FULL_NAME TEXT NOT NULL,
   EMAIL TEXT NOT NULL,
);


CREATE TABLE POST(
   ID INT PRIMARY KEY NOT NULL,
   FULL_NAME TEXT NOT NULL,
   EMAIL TEXT NOT NULL,
);